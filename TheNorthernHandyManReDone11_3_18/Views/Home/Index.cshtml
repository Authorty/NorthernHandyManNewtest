@{
    ViewBag.Title = "Home Page";
}
<div class="hero" id="highlighted">
    <div class="inner">
        <div id="highlighted-slider" class="container">
            <hr />
            <div class="item-slider" data-toggle="owlcarousel" data-owlcarousel-settings='{"singleItem":true, "navigation":true, "transitionStyle":"fadeUp", "autoPlay":true}'>
                <!--Slideshow content-->
                <!--Slide 1-->
                <div class="item">
                    <div class="row">
                        <div class="col-md-6 text-right-md item-caption">
                            <h4 style="padding-left:2em;">
                                <span class="text-primary">Servicing North Eastern Wisconsin</span> for over 30 Years!
                                </h4>

                            <p style="padding-left:2em;">

                                Have something small or large in mind, to transform your yard, or Business into a beautiful oasis?
                                Look no further; let our years of experience help you make your dreams a reality.
                                We specialize in residential and Commercial Communities.

                            </p>
                                <h2 class="text-success">
                                    <b>
                                        Quality matters when experience shines brightest.
                                    </b>
                                </h2>
</div>
                        <div class="col-md-6 hidden-xs">
                            <img src="~/img/Filler_ExtraImages/Equipment.jpg" alt="Slide 2" class="center-block img-responsive">
                        </div>
                    </div>
                </div>

                <!--Slide 2-->

                <div class="item">
                    <div class="row">
                        <div class="col-md-6 col-md-push-6 item-caption">
                            <h2 class="h1 text-weight-light, text-primary">
                                Summer Season: <br />Landscape Needs
                            </h2>

                            <h4 style="padding-right:2em;">
                               

                                Landscaping, Excavating,  Retaining Walls,
                                with over 30 years of experience there is so much we can do for you.

                            </h4>
                            <p style="padding-right:2em;">
                                Being in the business for as long as we have, we have the skills to tame a widly
                                overgrown to a yard and change it into something you can be proud of.
                            </p>
                            <a href=@Url.Action("Contact","Home") class="btn btn-more btn-lg i-right">Contact Us Now <i class="fa fa-plus"></i></a>
                        </div>
                        <div class="col-md-6 col-md-pull-6 hidden-xs">

                            <img src="~/img/Filler_ExtraImages/FullStumpRemoval.jpg" alt="Slide 1" class="center-block img-responsive">
                        </div>
                    </div>
                    
                </div>
                <!--Slide 3-->
                <div class="item">
                    <div class="row">
                        <div class="col-md-6 text-right-md item-caption">
                            <h2 class="h1 text-weight-light">
                                <span class="text-primary">
                                    Wisconsin Winter Season:
                                    <br /> Residental & Commercial -Snow Removal
                                </span>
                            </h2>

                            <h4 style="padding-left:2em;">
                                Snow is an incredible hassle for homeowners and business owners across Wisconsin.
                            </h4>
                            <p style="padding-left:2em;">
                                Depend on us to give you fast,
                                efficient, and reliable commercial and residential snow removal services.
                                We'll work with you to fit your budget and needs.
                            </p>
                            <a href=@Url.Action("AllPhotos","Services") class="btn btn-more btn-lg"><i class="fa fa-plus"></i> Learn More</a>
                        </div>
                        <div class="col-md-6 hidden-xs">
                            <img src="~/img/Filler_ExtraImages/seasons.jpg" alt="Slide 2" class="center-block img-responsive">
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <hr />
</div>
<div class="container">
    <div class="col-md-4">
        <h2 class="text-success">
            *FireWood For Sale*
        </h2>
        
            <p class="text-success">
                <small>
                    $60/Face Cord of mixed hardwood.<br />
                    Pick-Up or Delivery<br />
                </small>
            </p>

            <br />

            <p class="text-success">
                <small>
                    <strong>*Free* </strong> Delivery on purchases of 3-6 Face Cords within a 25 mile radius of New London WI,
                </small>
            </p>
            @Html.ActionLink("More »", "FireWood", "Home", new { area = "" }, new { @class = "btn btn-default btn-xs" })

        </div>
    <div class="text-primary"> <h2>Positions available for FALL and WINTER Season.</h2>
    <p class="text-primary">
         Located in Fox Valley Area. <br />
         Full Time and Part Time Positions available.<br />
         Please contact Northern Handyman 9202775947
</p>@Html.ActionLink("More »", "Employment", "Home", new { area = "" }, new { @class = "btn btn-default btn-xs" })
    </div>
</div>




